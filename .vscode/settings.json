{
"python.linting.pylintArgs": [
    "--enable",
    "F,E",
    "--rcfile",
    "${workspaceFolder}/lint-configs-python/python/pylintrc"
  ],
  "python.linting.mypyCategorySeverity.error": "Error",
  "python.linting.mypyCategorySeverity.note": "Warning",
  "python.linting.pylintCategorySeverity.warning": "Warning",
  "python.linting.pylintCategorySeverity.error": "Error",
  "python.linting.pylintCategorySeverity.convention": "Warning",
  "python.linting.pylintCategorySeverity.fatal": "Error",
  "python.linting.pylintCategorySeverity.refactor": "Warning",
  "python.analysis.completeFunctionParens": true,
  "python.analysis.indexing": true,
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true,
  "python.tensorBoard.logDirectory": "runs",
  "python.languageServer": "Pylance",
  "python.linting.prospectorEnabled": false,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.fixAll": ["source.convertImportFormat", "source.unusedImports"],
  "python.analysis.diagnosticSeverityOverrides": {
    "reportUnusedImport": "none",
    "reportMissingImports": "error",
    "reportImportCycles": "error",
    "reportUnusedVariable": "none",
    "reportMissingTypeStubs": "none",
    "reportUnknownMemberType": "none",
    "reportUnusedFunction": "warning",
    "reportUnusedClass": "warning",
  },
  "editor.formatOnSave": true,
  "python.linting.lintOnSave": true,
  "[python]": {
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false,
    "editor.formatOnSaveMode": "file",
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "python.formatting.provider": "none",
  "python.formatting.autopep8Args": [
    "--indent-size=4",
    "--max-line-length",
    "160",
    "--ignore=E111, E114, E402, E501"
  ],
  "black-formatter.args": [
    "--config",
    "pyproject.toml",
    "--force-exclude",
    "site-packages"
  ],
  "isort.args": [
    "--settings-file",
    "pyproject.toml",
    "--extend-skip-glob",
    "*/site-packages/*",
    "--profile",
    "black"
  ],
  "python.defaultInterpreterPath": "~/.pyenv/versions/anaconda3-2022.05/envs/fastapi_pytorch_postgresql_sandbox3/bin/python3",
  "output.smartScroll.enabled": true,
  "files.exclude": {
    ".idea/": "explorerExcludedFiles",
    ".vscode/": "explorerExcludedFiles",
    "__pycache__/": "explorerExcludedFiles",
    "*.py[cod]": "explorerExcludedFiles",
    "*$py.class": "explorerExcludedFiles",
    "*.so": "explorerExcludedFiles",
    ".Python": "explorerExcludedFiles",
    "build/": "explorerExcludedFiles",
    "develop-eggs/": "explorerExcludedFiles",
    "dist/": "explorerExcludedFiles",
    "downloads/": "explorerExcludedFiles",
    "eggs/": "explorerExcludedFiles",
    ".eggs/": "explorerExcludedFiles",
    "lib/": "explorerExcludedFiles",
    "lib64/": "explorerExcludedFiles",
    "parts/": "explorerExcludedFiles",
    "sdist/": "explorerExcludedFiles",
    "var/": "explorerExcludedFiles",
    "wheels/": "explorerExcludedFiles",
    "share/python-wheels/": "explorerExcludedFiles",
    "*.egg-info/": "explorerExcludedFiles",
    ".installed.cfg": "explorerExcludedFiles",
    "*.egg": "explorerExcludedFiles",
    "MANIFEST": "explorerExcludedFiles",
    "*.manifest": "explorerExcludedFiles",
    "*.spec": "explorerExcludedFiles",
    "pip-log.txt": "explorerExcludedFiles",
    "pip-delete-this-directory.txt": "explorerExcludedFiles",
    "htmlcov/": "explorerExcludedFiles",
    ".tox/": "explorerExcludedFiles",
    ".nox/": "explorerExcludedFiles",
    ".coverage": "explorerExcludedFiles",
    ".coverage.*": "explorerExcludedFiles",
    ".cache": "explorerExcludedFiles",
    "nosetests.xml": "explorerExcludedFiles",
    "coverage.xml": "explorerExcludedFiles",
    "*.cover": "explorerExcludedFiles",
    "*.py,cover": "explorerExcludedFiles",
    ".hypothesis/": "explorerExcludedFiles",
    ".pytest_cache/": "explorerExcludedFiles",
    "cover/": "explorerExcludedFiles",
    "*.mo": "explorerExcludedFiles",
    "*.pot": "explorerExcludedFiles",
    "*.log": "explorerExcludedFiles",
    "local_settings.py": "explorerExcludedFiles",
    "db.sqlite3": "explorerExcludedFiles",
    "db.sqlite3-journal": "explorerExcludedFiles",
    "instance/": "explorerExcludedFiles",
    ".webassets-cache": "explorerExcludedFiles",
    ".scrapy": "explorerExcludedFiles",
    "docs/_build/": "explorerExcludedFiles",
    ".pybuilder/": "explorerExcludedFiles",
    "target/": "explorerExcludedFiles",
    ".ipynb_checkpoints": "explorerExcludedFiles",
    "profile_default/": "explorerExcludedFiles",
    "ipython_config.py": "explorerExcludedFiles",
    "__pypackages__/": "explorerExcludedFiles",
    "celerybeat-schedule": "explorerExcludedFiles",
    "celerybeat.pid": "explorerExcludedFiles",
    "*.sage.py": "explorerExcludedFiles",
    ".env": "explorerExcludedFiles",
    ".venv": "explorerExcludedFiles",
    "env/": "explorerExcludedFiles",
    "venv/": "explorerExcludedFiles",
    "ENV/": "explorerExcludedFiles",
    "env.bak/": "explorerExcludedFiles",
    "venv.bak/": "explorerExcludedFiles",
    ".spyderproject": "explorerExcludedFiles",
    ".spyproject": "explorerExcludedFiles",
    ".ropeproject": "explorerExcludedFiles",
    "site": "explorerExcludedFiles",
    ".mypy_cache/": "explorerExcludedFiles",
    ".dmypy.json": "explorerExcludedFiles",
    "dmypy.json": "explorerExcludedFiles",
    ".pyre/": "explorerExcludedFiles",
    ".pytype/": "explorerExcludedFiles",
    "cython_debug/": "explorerExcludedFiles"
  },
  "json.schemas": [
    {
        "fileMatch": [
            "*.meta.yaml"
        ],
        "url": "https://raw.githubusercontent.com/microsoft/vscode-python/main/schemas/conda-meta.json"
    }
  ],
  "python.linting.mypyEnabled": true,
  // "python.linting.mypyCategorySeverity.note": "Information",
  // "python.linting.mypyCategorySeverity.error": "Error",
  "python.linting.mypyArgs": [
    // "--follow-imports=silent",
    // "--ignore-missing-imports",
    "--show-column-numbers",
    "--no-pretty",
    // "--exclude '(contrib|fastapi_pytorch_postgresql_sandbox*)/$'",
    "--config-file=${workspaceFolder}/lint-configs-python/python/mypy.ini"
  ],
  "isort.check": true,
}
